<h1>Create A Song</h1>
<%= form_for @song do |f|%>
  Title: <%= f.text_field :title %>
  <br>
  Artist: <%= f.text_field :artist_name %>
  <br>
  <%= f.label :genre %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %> <br>

  Notes: <input name="song[note_contents][]" type="text" id="song_notes_1">
  <br>
  Notes: <input name="song[note_contents][]" type="text" id="song_notes_2">
  <br>
  <%= f.submit %>
  <% end %>


